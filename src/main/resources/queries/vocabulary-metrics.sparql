PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX voaf: <http://purl.org/vocommons/voaf#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX dct:  <http://purl.org/dc/terms/>
PREFIX vann: <http://purl.org/vocab/vann/>

# Finds all datasets in which the vocabulary is used
SELECT DISTINCT ?dataset ?datasetLabel ?occurrences{
	?vocab voaf:usageInDataset ?usage.
	?usage voaf:inDataset ?dataset.
	?usage voaf:occurrences ?occurrences.
	?dataset rdfs:label ?datasetLabel.
} ORDER BY DESC(?occurrences)